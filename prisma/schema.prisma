generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pet {
  id       Int     @id @default(autoincrement())
  name     String
  age      Int
  category String
  breed    String
  state    String
  city     String
  contact  String
  image    String?
}
